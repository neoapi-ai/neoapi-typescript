{
    "name": "neoapi-sdk",
    "version": "1.0.2",
    "description": "Integrate neoapi.ai LLM Analytics with your LLM pipelines.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/neoapi-ai/neoapi-typescript.git"
    },
    "keywords": [
        "neoapi",
        "llm",
        "analytics",
        "sdk",
        "typescript"
    ],
    "author": "NeoAPI",
    "license": "Apache-2.0",
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^20.11.0",
        "axios-mock-adapter": "^1.22.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.5",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "axios": "^1.7.7"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/**/*.test.ts"
        ],
        "transform": {
            "^.+\\.tsx?$": [
                "ts-jest",
                {
                    "tsconfig": "tsconfig.jest.json"
                }
            ]
        }
    }
}
